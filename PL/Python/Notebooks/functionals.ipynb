    "def sum_dig_pow(a, b): # range(a, b + 1) will be studied by the function\n",
    "    powered_sum = lambda x: sum([v**(i+1) for i,v in enumerate(map(lambda x: int(x), list(str(x))))])\n",
    "    return [i for i in range(a,b+1) if powered_sum(i)==i]"
   ]
  }
